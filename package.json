{
  "name": "app",
  "version": "0.0.0",
  "description": "CMS Server",
  "main": "lib/index",
  "config": {
    "unsafe-perm": true
  },
  "scripts": {
    "clean": "rimraf dist/* && rimraf lib/*",
    "prebuild": "npm run clean -s",
    "build": "npm run prebuild && gulp && npm run build-server",
    "build-server": "babel --stage 0 --optional runtime src --out-dir lib",
    "test": "lab -T node_modules/lab-babel -t 90 -S -c -D -v",
    "start": "node lib/index.js",
    "lint": "eslint ./ || true",
    "babel": "babel --stage 0",
    "dev": "NODE_ENV=dev gulp server:dev",
    "postinstall": "npm run build"
  },
  "author": "Brian Bland",
  "license": "BSD-2-Clause",
  "dependencies": {
    "babel": "^5.6.14",
    "babel-core": "^5.8.22",
    "boom": "^2.8.0",
    "handlebars": "^3.0.3",
    "hapi": "^8.8.1",
    "immutable": "^3.7.5",
    "isomorphic-fetch": "^2.1.1",
    "joi": "^6.6.1",
    "lodash": "^3.10.1",
    "marked": "^0.3.5",
    "material-ui": "^0.11.0",
    "methods": "^1.1.1",
    "mime": "^1.3.4",
    "qs": "^5.0.0",
    "react": "^0.13.3",
    "react-marked": "^0.3.1",
    "react-mixin": "^1.4.0",
    "react-redux": "^0.2.2",
    "react-router": "1.0.0-beta3",
    "redux": "1.0.0-rc",
    "redux-promise": "^0.5.0",
    "redux-thunk": "^0.1.0",
    "superagent": "^1.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^4.0.5",
    "babel-runtime": "^5.8.24",
    "babelify": "^6.3.0",
    "browser-sync": "^2.8.2",
    "browserify": "^11.0.1",
    "chai": "^3.2.0",
    "debug": "^2.2.0",
    "gulp": "^3.9.0",
    "gulp-minify-css": "^1.2.1",
    "gulp-nodemon": "^2.0.4",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.0.4",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-util": "^3.0.6",
    "lab": "^5.14.0",
    "lab-babel": "^1.1.0",
    "node-sass": "^3.3.2",
    "nodemon": "^1.4.1",
    "parallelshell": "^2.0.0",
    "reactify": "^1.1.1",
    "require-dir": "^0.3.0",
    "rimraf": "^2.4.3",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.3.1"
  },
  "repository": {
    "description":"",
    "version":"0.0.1",
    "private": true
  }
